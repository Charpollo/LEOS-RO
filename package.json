{
  "name": "leos-first-orbit",
  "version": "1.0.0",
  "description": "LEOS First Orbit Project",
  "private": true,
  "scripts": {
    "clean": "rimraf frontend/dist",
    "build": "webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:prod:analyze": "NODE_ENV=production webpack --config webpack.config.prod.js --analyze",
    "watch": "webpack --config webpack.config.js --watch",
    "start": "./run-frontend.sh",
    "start:prod": "NODE_ENV=production ./run-frontend.sh",
    "dev": "./run-frontend.sh --clean --port 8000",
    "optimize:models": "node scripts/optimize-models.js",
    "optimize:models:replace": "node scripts/optimize-models.js --replace"
  },
  "dependencies": {
    "@babylonjs/core": "^6.27.1",
    "@babylonjs/gui": "^6.27.1",
    "@babylonjs/loaders": "^6.27.1",
    "@babylonjs/materials": "^6.27.1",
    "satellite.js": "^6.0.1"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "javascript-obfuscator": "^4.1.1",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.2",
    "terser": "^5.43.1",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-obfuscator": "^3.5.1"
  }
}
